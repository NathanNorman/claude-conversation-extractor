[run]
source = .
omit = 
    venv/*
    tests/*
    setup.py
    demo_interactive.py
    */site-packages/*
    */__pycache__/*
    
[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
    except ImportError:
    pass
    except Exception:

[html]
directory = htmlcov